{
    "extends": "../tslint.json",
    "rules": {
        "no-redundant-jsdoc": false,
        "max-line-length": [
            false
        ],
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-console": true
    }
}
